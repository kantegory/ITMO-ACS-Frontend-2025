{
  "name": "profiles-service",
  "version": "1.0.0",
  "main": "dist/app.js",
  "scripts": {
    "dev": "npm run tsoa:routes && npm run tsoa:spec && ts-node-dev --respawn --transpile-only src/app.ts",
    "build": "tsc",
    "start": "node dist/app.js",
    "tsoa:routes": "tsoa routes",
    "tsoa:spec": "tsoa spec",
    "typeorm": "typeorm-ts-node-commonjs",
    "typeorm:js": "typeorm",
    "migration:generate": "npm run typeorm -- migration:generate -d src/config/data-source.ts src/migrations/Init",
    "migration:run": "npm run typeorm -- migration:run -d src/config/data-source.ts",
    "migration:revert": "npm run typeorm -- migration:revert -d src/config/data-source.ts",
    "migration:run:prod": "npm run typeorm:js -- migration:run -d dist/config/data-source.js",
    "migration:revert:prod": "npm run typeorm:js -- migration:revert -d dist/config/data-source.js"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "http-errors": "^2.0.0",
    "pg": "^8.15.0",
    "reflect-metadata": "^0.2.2",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.22",
    "jsonwebtoken": "^9.0.2",
    "@tsoa/runtime": "^6.6.0",
    "tsoa": "^6.6.0",
    "tsyringe": "^4.8.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^22.14.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsoa": "^6.6.0",
    "typescript": "^5.8.3"
  }
}

